{
  "manifest_version": 2,
  "name": "GraphQL Inspector",
  "description": "Inspect GraphQL requests.",
  "version": "1.0.0",
  "minimum_chrome_version": "43",
  "devtools_page": "devtools.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}